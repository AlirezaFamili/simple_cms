<% @page_title = "Pages New" %>
<%= link_to("<< Back to List", {:action => 'index',:subject_id=>@subject.id}, :class => 'back-link') %>

<div class="pages new">
  <h2>Create Page</h2>

  <%= form_for(:page, :url => {:action => 'create',:subject_id=>@subject.id}) do |f1| %>

    <%= render(:partial => "pages/form",:locals => {:f => f1}) %>

    <div class="form-buttons">
      <%= submit_tag("Create Page") %>
    </div>

  <% end %>
</div>
